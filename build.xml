<?xml version="1.0" ?><project default="main">	<property name="projectName" value="Random" />	<property name="jarMessage" value="Building the .jar file." />	<property name="packageDir" value="net/codehobby" />	<property name="packageBaseDir" value="net" />	<property name="srcDir" value="src" />	<property name="outputDir" value="." />	<property name="classDir" value="bin" />	<target name="main" depends="compile, compress">		<echo>			${jarMessage}		</echo>	</target>	<target name="compile">		<javac includeantruntime="false" destdir="." srcdir="${srcDir}/${packageDir}" classpath="gson-2.3.jar" debug="on"/>	</target>	<target name="compress">		<jar jarfile="${outputDir}/${projectName}.jar" basedir="." includes="${packageDir}/*/*.class gson-2.3.jar">			<manifest>				<attribute name="Main-Class" value="${packageDir}/random/Random"/>				<attribute name="Class-Path" value="${packageDir}/*.class gson-2.3.jar"/>			</manifest>		</jar>		<chmod file="${outputDir}/${projectName}.jar" perm="ugo+rx"/>	</target>	<target name="clean">		<delete file="${outputDir}/${projectName}.jar"/>		<delete dir="${packageBaseDir}"/>	</target></project>