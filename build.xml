<?xml version="1.0" ?><project default="main">	<property name="projectName" value="Random" />	<property name="jarMessage" value="Building the .jar file." />	<property name="srcDir" location="src/net/codehobby" />	<property name="outputDir" location="." />	<property name="classDir" location="bin" />	<target name="main" depends="compile, compress">		<echo>			${jarMessage}		</echo>	</target>	<target name="compile">		<javac includeantruntime="false" srcdir="${srcDir}" destdir="${classDir}" classpath="gson-2.3.jar" debug="on"/>	</target>	<target name="compress">		<jar jarfile="${outputDir}/${projectName}.jar" basedir="." includes="${classDir}/*.class gson-2.3.jar">			<manifest>				<attribute name="Main-Class" value="Random"/>				<attribute name="Class-Path" value="gson-2.3.jar"/>			</manifest>		</jar>	</target></project>